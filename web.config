<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <clear />
                <add value="index.html" />
                <add value="index.php" />
                <add value="Default.asp" />
                <add value="Default.htm" />
                <add value="iisstart.htm" />
                <add value="default.aspx" />
            </files>
        </defaultDocument>
	<!--<globalization requestEncoding="utf-8" responseEncoding="utf-8" fileEncoding="utf-8" />-->
	<httpProtocol>
		<customHeaders>
			<remove name="Vary"/>
			<add name="Vary" value="Accept-Encoding"/>
		</customHeaders>
	</httpProtocol>
	<urlCompression doDynamicCompression="true" doStaticCompression="true" dynamicCompressionBeforeCache="true"/>
    </system.webServer>
    <location path="img">
    	<system.webServer>
		<staticContent>
			<clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365:00:00:00"/>
      		</staticContent>
    	</system.webServer>
    </location>
    <location path="files">
        <system.webServer>
                <staticContent>	
		                <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365:00:00:00"/>
                </staticContent>
        </system.webServer>
    </location>
    <location path="video">
        <system.webServer>
                <staticContent>	
		                <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365:00:00:00"/>
                </staticContent>
        </system.webServer>
    </location>
</configuration>
